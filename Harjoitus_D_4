using System;

namespace Harjotus_D
{
    class Program
    {
        static void Main(string[] args)
        {
            double[] luku = { 0, 0 };
            double jakoj = 0;
            for (int i = 0; i < 2; i++)
            {
                Console.WriteLine("{0}. Anna luku", i + 1);
                luku[i] = double.Parse(Console.ReadLine());
            }
            jakoj = luku[0] % luku[1];
            Console.WriteLine("jakojäännös on {0}",jakoj);
        }
    }
}
