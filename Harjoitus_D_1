using System;

namespace Harjotus_D
{
    class Program
    {
        static void Main(string[] args)
        {
            int lasku = 0;
            for (int i = 0; i < 2; i++)
            {
                Console.WriteLine("{0}. Anna luku", i + 1);
                lasku += int.Parse(Console.ReadLine());
            }
            Console.WriteLine(lasku);
        }
    }
}
